<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anandy.batterychargetracker">

    <application
        android:name=".BatteryChargeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BatteryChargeTracker">
        <activity
            android:name=".ui.main.MainActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.BatteryChargeTracker.AppBarOverlay">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.register.RegisterCharge"
            android:label="@string/title_activity_register_charge">
        </activity>
    </application>

</manifest>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anandy.motoechargetracker">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:name="com.anandy.motoechargetracker.MotoEChargeTrackerApp"
        android:allowBackup="true"
        android:icon="@mipmap/motoe_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/motoe_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BatteryChargeTracker"
        android:versionCode="1"
        android:versionName="First Release">

        <activity android:name=".ui.MainNavHostActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>